{
  "name": "burlap",
  "private": true,
  "version": "0.0.2",
  "main": "app/index.js",
  "description": "Boilerplate Electron + Vite + Svelte",
  "license": "MIT",
  "scripts": {
    "start": "pnpm run build && pnpm run electron:start",
    "dev": "concurrently -k \"vite -p 3000\" \"npm run electron:dev\"",
    "build": "pnpm run compile && vite build",
    "electron:dev": "pnpm run compile && cross-env IS_DEV=true electron-forge start",
    "electron:build": "pnpm run compile && electron-forge make",
    "electron:package": "pnpm run compile && electron-forge package",
    "compile": "swc ./app/index.ts -o ./app/index.js"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.5",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "4.3.4",
    "@tanstack/react-query-devtools": "^4.3.4",
    "@types/marked": "^4.0.7",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^6",
    "isomorphic-dompurify": "^0.22.0",
    "marked": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pdf": "^5.7.2",
    "showdown": "^2.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0",
    "@electron-forge/maker-deb": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0",
    "@electron-forge/maker-zip": "^6.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@tailwindcss/typography": "^0.5.7",
    "@tauri-apps/cli": "^1.1.1",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "electron": "^21.2.2",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "vite": "^3.2.2",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}